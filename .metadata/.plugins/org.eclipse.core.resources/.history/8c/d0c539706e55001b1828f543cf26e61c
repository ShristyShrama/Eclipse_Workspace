package OOPConceptPart2;

public class HSBCBank implements USBank{
		
		public void credit()
		{
			System.out.println("USBank--credit");
		}
		
		public void debit()
		{
			System.out.println("USBank--debit");
		}
		
		public void transfermoney()
		{
			System.out.println("USBank--tranfermoney");
		}

}
